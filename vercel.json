{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
      "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "server.js" },
      { "src": "/createUser", "dest": "server.js" },
      { "src": "/user", "dest": "server.js" },
      { "src": "/user/:userId", "dest": "server.js" },
      { "src": "/user/:userId", "dest": "server.js" },
      { "src": "/user/:userId", "dest": "server.js" }
    ]
  }
  